<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://j2ee.netbeans.org/wsdl/Test/src/init" xmlns:ns2="http://enterprise.netbeans.org/bpel/Test/test" xmlns:ns3="http://j2ee.netbeans.org/wsdl/Test/src/TestOutboundPostCode" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
    <service-assembly>
        <identification>
            <name>TestCA</name>
            <description>Represents the Service Assembly of TestCA</description>
        </identification>
        <service-unit>
            <identification>
                <name>TestCA-Test</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>Test.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>TestCA-sun-rest-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-rest-binding.jar</artifacts-zip>
                <component-name>sun-rest-binding</component-name>
            </target>
        </service-unit>
        <connections>
            <connection>
                <consumer endpoint-name="RestInboundWSDL_InboundPort" service-name="ns1:RestInboundService"/>
                <provider endpoint-name="RestInboundPortTypeRole_myRole" service-name="ns2:PartnerLink2"/>
            </connection>
            <connection>
                <consumer endpoint-name="RestOutboundPortTypeRole_partnerRole" service-name="ns2:PartnerLink1"/>
                <provider endpoint-name="RestOutboundWSDL_OutboundPort" service-name="ns3:RestOutboundService"/>
            </connection>
        </connections>
        <route-types xmlns="http://www.open-esb.net/xml/ns/jbi">
            <source endpoint-name="RestInboundWSDL_InboundPort" mode="local" routing-rule="default" service-name="ns1:RestInboundService">direct</source>
            <source endpoint-name="RestOutboundPortTypeRole_partnerRole" mode="local" routing-rule="default" service-name="ns2:PartnerLink1">direct</source>
        </route-types>
        <interface-mapping xmlns="http://www.open-esb.net/xml/ns/jbi">
            <implementation>
                <by-provider endpoint-name="RestInboundPortTypeRole_myRole" service-name="ns2:PartnerLink2"/>
                <interface name="ns1:RestInboundPortType"/>
            </implementation>
            <implementation>
                <by-provider endpoint-name="RestOutboundWSDL_OutboundPort" service-name="ns3:RestOutboundService"/>
                <interface name="ns3:RestOutboundPortType"/>
            </implementation>
        </interface-mapping>
    </service-assembly>
</jbi>
